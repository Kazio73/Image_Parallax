<?xml version="1.0"?>
<schema version="0.3">
	<table name="btImageParallax">
		<field name="bID" type="I">
			<key />
			<unsigned />
		</field>
		<field name="fID" size="10" type="I">
			<default value="0" />
			<unsigned />
		</field>
		<field name="height" size="3" type="I">
			<default value="20" />
		</field>
		<field name="speed" precision="1" scale="1" type="F">
			<default value="0.5" />
		</field>

		<field name="alt" type="X">
			<default value="" />
		</field>
	</table>
</schema>
